        "electron": "^9.1.0",
        "electron-builder": "^22.7.0"
    },
    "build": {
        "appId": "com.joseviniciusnunes.realm-data-view",
        "mac": {
            "category": "tools"
        },
        "win": {
            "target": [
                "portable"
            ]
        }
    }
